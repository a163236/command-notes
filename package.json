{
  "publisher": "a163236",
  "repository": {
    "url": "https://github.com/a163236/command-notes",
    "type": "git",
    "branch": "main"
  },
  "name": "command-notes",
  "displayName": "command-notes",
  "description": "A VSCode extension that memorize your favorite commands and execute them easily",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "command-notes-container",
          "title": "Command Notes",
          "icon": "$(terminal)"
        }
      ]
    },
    "views": {
      "command-notes-container": [
        {
          "id": "commandNotes",
          "name": "My Commands",
          "icon": "$(terminal)"
        }
      ]
    },
    "commands": [
      {
        "command": "command-notes.addCommand",
        "title": "Add Command",
        "icon": "$(add)"
      },
      {
        "command": "command-notes.addGroup",
        "title": "Add Group",
        "icon": "$(new-folder)"
      },
      {
        "command": "command-notes.executeCommand",
        "title": "Execute Command",
        "icon": "$(play)"
      },
      {
        "command": "command-notes.copyCommand",
        "title": "Copy Command",
        "icon": "$(copy)"
      },
      {
        "command": "command-notes.editCommand",
        "title": "Edit Command",
        "icon": "$(edit)"
      },
      {
        "command": "command-notes.renameGroup",
        "title": "Rename Group",
        "icon": "$(edit)"
      },
      {
        "command": "command-notes.deleteNode",
        "title": "Delete",
        "icon": "$(trash)"
      },
      {
        "command": "command-notes.moveUp",
        "title": "Move Up",
        "icon": "$(arrow-up)"
      },
      {
        "command": "command-notes.moveDown",
        "title": "Move Down",
        "icon": "$(arrow-down)"
      },
      {
        "command": "command-notes.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "command-notes.export",
        "title": "Export Commands",
        "icon": "$(export)"
      },
      {
        "command": "command-notes.import",
        "title": "Import Commands",
        "icon": "$(cloud-download)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "command-notes.addCommand",
          "when": "view == commandNotes",
          "group": "navigation@1"
        },
        {
          "command": "command-notes.addGroup",
          "when": "view == commandNotes",
          "group": "navigation@2"
        },
        {
          "command": "command-notes.refresh",
          "when": "view == commandNotes",
          "group": "navigation@3"
        },
        {
          "command": "command-notes.export",
          "when": "view == commandNotes",
          "group": "navigation@4"
        },
        {
          "command": "command-notes.import",
          "when": "view == commandNotes",
          "group": "navigation@5"
        }
      ],
      "view/item/context": [
        {
          "command": "command-notes.executeCommand",
          "when": "view == commandNotes && viewItem == commandItem",
          "group": "inline@1"
        },
        {
          "command": "command-notes.copyCommand",
          "when": "view == commandNotes && viewItem == commandItem",
          "group": "inline@2"
        },
        {
          "command": "command-notes.deleteNode",
          "when": "view == commandNotes && viewItem == commandItem",
          "group": "inline@3"
        },
        {
          "command": "command-notes.deleteNode",
          "when": "view == commandNotes && viewItem == commandGroup",
          "group": "inline@1"
        },
        {
          "command": "command-notes.executeCommand",
          "when": "view == commandNotes && viewItem == commandItem",
          "group": "1_actions@1"
        },
        {
          "command": "command-notes.copyCommand",
          "when": "view == commandNotes && viewItem == commandItem",
          "group": "1_actions@2"
        },
        {
          "command": "command-notes.addCommand",
          "when": "view == commandNotes && viewItem == commandGroup",
          "group": "2_add@1"
        },
        {
          "command": "command-notes.addGroup",
          "when": "view == commandNotes && viewItem == commandGroup",
          "group": "2_add@2"
        },
        {
          "command": "command-notes.editCommand",
          "when": "view == commandNotes && viewItem == commandItem",
          "group": "3_edit@1"
        },
        {
          "command": "command-notes.renameGroup",
          "when": "view == commandNotes && viewItem == commandGroup",
          "group": "3_edit@1"
        },
        {
          "command": "command-notes.moveUp",
          "when": "view == commandNotes",
          "group": "4_move@1"
        },
        {
          "command": "command-notes.moveDown",
          "when": "view == commandNotes",
          "group": "4_move@2"
        },
        {
          "command": "command-notes.deleteNode",
          "when": "view == commandNotes",
          "group": "5_delete@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}